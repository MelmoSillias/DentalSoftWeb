{% extends 'appbase.html.twig' %}

{% block title %}Consultations Clôturées{% endblock %}



{% block main %}
<div class="container-fluid px-4">
    <h1 class="mt-4">Consultations Clôturées</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item active"><i class="fas fa-home"></i></li>
        <li class="breadcrumb-item active">Consultations Clôturées</li>
    </ol>

    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">
                <i class="fas fa-table me-1"></i> Liste des consultations clôturées
            </h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table id="closedConsultationsTable" class="table table-bordered table-striped my-4" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>N°</th>
                            <th>Date</th>
                            <th>Patient</th>
                            <th>Médecin</th>
                            <th>Salle</th> 
                            <th>Actions</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block javascripts %} 
    <script src="{{ asset('js/closedconsult.js') }}"></script>
{% endblock %}
